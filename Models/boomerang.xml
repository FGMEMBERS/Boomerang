      <y1-m>  0.725 </y1-m>
      <z1-m> -0.070 </z1-m>
      <x2-m>  4.070 </x2-m>
      <y2-m>  0.725 </y2-m>
      <z2-m>  0.966 </z2-m>
    </axis>
  </animation>

  <!-- Ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <property>/surface-positions/left-aileron-pos-norm</property>
    <factor> 15 </factor>
    <axis>
