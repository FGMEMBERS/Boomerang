      <entry><ind> 1.0 </ind><dep> 90 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -0.88194 </x1-m>
      <y1-m> -1.35972 </y1-m>
      <z1-m> -0.69415 </z1-m>
      <x2-m> -0.24976 </x2-m>
      <y2-m> -1.35972 </y2-m>
      <z2-m> -0.70149 </z2-m>
    </axis>
  </animation>
  
