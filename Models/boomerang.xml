  <model>
    <path>Aircraft/Boomerang/Models/Effects/ground/ground1.xml</path>
    <offsets>
      <x-m> -3.11344 </x-m>
      <y-m> -0.49500 </y-m>
      <z-m> -1.31916 </z-m>
    </offsets>
  </model>
  
