      <y-m> -1.51200 </y-m>
      <z-m> -1.29157 </z-m>
    </offsets>
  </model>
  
  <!-- Roue droite -->
  <model>
    <path>Aircraft/Boomerang/Models/Effects/ground/ground2.xml</path>
    <offsets>
      <x-m> -0.79180 </x-m>
      <y-m>  0.72500 </y-m>
      <z-m> -1.29157 </z-m>
    </offsets>
  </model>
  
  <!-- Tip trails -->
