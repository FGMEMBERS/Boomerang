    <path>Aircraft/Boomerang/Models/shadow.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> -1.14 </z-m>
    </offsets>
  </model>
  
  <!-- Cranking effects -->
  <model>
    <path>Aircraft/Boomerang/Models/Effects/cranking/crankingL.xml</path>
    <offsets>
      <x-m> -1.24571 </x-m>
      <y-m> -1.08064 </y-m>
      <z-m> -0.26227 </z-m>
      <pitch-deg> 15 </pitch-deg>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Boomerang/Models/Effects/cranking/crankingR.xml</path>
    <offsets>
      <x-m> -3.24876 </x-m>
      <y-m>  0.89903 </y-m>
      <z-m> -0.81902 </z-m>
      <pitch-deg> -15 </pitch-deg>
    </offsets>
  </model>
  
