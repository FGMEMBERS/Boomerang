    <type>spin</type>
    <object-name>Helice2Complete</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
    <property>/engines/engine[1]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -4.172 </x-m>
      <y-m> -1.512 </y-m>
      <z-m> -0.296 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <property>/surface-positions/elevator-pos-norm</property>
    <factor> 15 </factor>
    <center>
      <x-m>  4.291 </x-m>
      <y-m>  0.381 </y-m>
      <z-m> -0.123 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y> 
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>directionG</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 15 </factor>
    <axis>
      <x1-m>  3.932 </x1-m>
      <y1-m> -1.512 </y1-m>
      <z1-m> -0.070 </z1-m>
