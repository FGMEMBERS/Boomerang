  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>porteA2</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   0 </dep></entry>
      <entry><ind> 0.2 </ind><dep> -90 </dep></entry>
