    <factor> 15 </factor>
    <axis>
      <x1-m> -0.941 </x1-m>
      <y1-m>  1.397 </y1-m>
      <z1-m> -0.319 </z1-m>
      <x2-m> -1.808 </x2-m>
      <y2-m>  4.776 </y2-m>
      <z2-m>  0.008 </z2-m>
    </axis>
  </animation>

  <!-- Compression -->
  <animation>
    <name>CompressionAvant</name>
    <object-name>roueA</object-name>
    <object-name>axeAB</object-name>
  </animation>
  
  <animation>
